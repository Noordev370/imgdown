{
  "manifest_version": 3,
  "name": "ImgDown",
  "version": "1.0",
  "description": "Downloads images in the page",
  "homepage_url": "https://github.com/Noordev370/imgdown",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png"
  },
  "permissions": ["activeTab", "downloads", "tabs", "scripting"],
  "action": {
    "default_icon": "icons/icon32.png",
    "default_title": "imageDownload",
    "default_popup": "popup/popup.html"
  }
}
